{
  "name": "@davepwsmith/gatsby-source-flickr",
  "version": "2.0.1",
  "description": "A source plugin to retrieve photos from flickr",
  "author": "David Smith",
  "repository": "https://github.com/davepwsmith/gatsby-source-flickr.git",
  "files": [
    "dist",
    "gatsby-node.js"
  ],
  "types": "./dist/gatsby-node.d.ts",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "flickr",
    "photos"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "lint": "eslint ./src --ext .ts,.tsx",
    "lint:fix": "eslint ./src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "flickr-sdk": "^6.2.1"
  },
  "devDependencies": {
    "@types/node": "^18.16.16",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "gatsby": "^5.0.0",
    "gatsby-plugin-image": "^3.10.0",
    "gatsby-transformer-sharp": "^5.10.0"
  }
}
